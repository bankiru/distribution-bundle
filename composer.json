{
  "name": "bankiru/distribution-bundle",
  "description": "Distribution preparation",
  "homepage": "https://github.com/bankiru/distribution-bundle",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Boris Gorbylev",
      "email": "gorbylev@banki.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~5.6 || ~7.0",
    "symfony/expression-language": "~2.6 || ~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "composer/composer": "~1.0@dev",
    "fabpot/php-cs-fixer": "^1.0",
    "codeception/aspect-mock": "^1.0"
  },
  "scripts": {
    "fix-csc": [
      "php-cs-fixer fix -vvv src; exit 0"
    ]
  },
  "autoload": {
    "psr-4": {
      "Bankiru\\DistributionBundle\\": ["src","tests"]
    }
  }
}
